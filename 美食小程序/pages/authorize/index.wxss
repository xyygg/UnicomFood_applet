.app-container {
  position: absolute;
  width: 100%;
  height: 100%;
  background: #FFF;
}

.button {
  width: 90%!important;
  margin: 10px 3%;
  text-align: center;
  color: #FFF;
  background: #ff5151;
  border-color: #ff5151;
  font-size: 12px;
  padding: 5px 0!important;
  line-height: 30px;
  border-radius: initial;
  font-weight: initial;
  display: block;
  border-radius: 5px;
}

.logo {
  display: -webkit-box;
  -webkit-box-pack: center;
  -webkit-box-align: center;
  width:100%;
  margin: 10vw 0;
}
.t-a-c{text-align: center;}
.logo image {
  display: inline-block;
  width: 657rpx;
  height: 703rpx;
}