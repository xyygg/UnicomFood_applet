/* 客服按钮 */
.kefu{width:90rpx;height:90rpx;border-radius: 50%; overflow: hidden;position: fixed;right:3vw;bottom: 10vh;}
.kefu image{width: 100%;height:100%;border-radius: 50%;}
/* 客服二维码 */
.layer{width:100%;height:100%;background: rgba(0,0,0,0.8);position: fixed;top:0;left:0;display: flex;justify-content: center;align-items: center;overflow:hidden; visibility: hidden;opacity: 0;-webkit-transition: opacity 0.35s, visibility 0.35s;}
.layercontent{width:64%;background: #fff;border-radius: 30rpx;padding:15vw 40rpx 40rpx;position: relative;}
.active{visibility: visible;opacity: 1; -webkit-transition: opacity 0.35s; z-index: 1000}